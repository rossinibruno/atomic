{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      { "include": "modules/negotiations/certificates/*", "outDir": "dist" },
      { "include": "util/templates/*", "outDir": "dist" }
    ],
    "watchAssets": true
  }
}
